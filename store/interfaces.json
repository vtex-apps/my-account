{
  "my-account": {
    "component": "index",
    "required": [
      "my-account-pages",
      "my-account-defaultPage",
      "my-account-menu",
      "profile-display-container",
      "profile-input-container"
    ],
    "render": "client"
  },
  "my-account-defaultPage": {
    "component": "DefaultPage"
  },
  "my-account-pages": {
    "allowed": ["my-orders", "my-cards", "my-account-page"]
  },
  "my-account-page": {
    "component": "*",
    "extensibility": "public"
  },
  "my-account-menu": {
    "allowed": [
      "my-profile-link",
      "my-addresses-link",
      "my-orders-link",
      "my-cards-link",
      "my-account-link"
    ]
  },
  "my-account-link": {
    "component": "*",
    "extensibility": "public"
  },
  "my-addresses-link": {
    "component": "MyAddressesLink"
  },
  "my-profile-link": {
    "component": "MyProfileLink"
  },
  "profile-display-container": {
    "allowed": ["profile-display"]
  },
  "profile-display": {
    "component": "*",
    "extensibility": "public"
  },
  "profile-input-container": {
    "allowed": ["profile-input"]
  },
  "profile-input": {
    "component": "*",
    "extensibility": "public"
  }
}
