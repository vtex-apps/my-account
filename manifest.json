{
  "name": "my-account",
  "vendor": "vtex",
  "version": "1.3.1",
  "title": "My Account",
  "description": "",
  "mustUpdateAt": "2019-07-09",
  "dependencies": {
    "vtex.my-subscriptions": "1.x",
    "vtex.my-cards": "1.x",
    "vtex.my-orders-app": "3.x",
    "vtex.my-account-commons": "1.x",
    "vtex.store-graphql": "2.x",
    "vtex.styleguide": "9.x",
    "vtex.profile-form": "3.x",
    "vtex.country-codes": "2.x",
    "vtex.address-form": "4.x"
  },
  "builders": {
    "react": "3.x",
    "store": "0.x",
    "messages": "1.x",
    "node": "3.x"
  },
  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "settingsSchema": {
    "title": "My Account",
    "type": "object",
    "properties": {
      "profile": {
        "type": "object",
        "title": "Profile",
        "properties": {
          "showGenders": {
            "title": "Show non-binary genders when editing profile",
            "type": "boolean",
            "default": false
          }
        }
      },
      "addresses": {
        "type": "object",
        "title": "Addresses",
        "properties": {
          "useMap": {
            "title": "Use Google Maps to fill address data",
            "type": "boolean",
            "enum": [
              false,
              true
            ],
            "default": false
          }
        },
        "dependencies": {
          "useMap": {
            "oneOf": [
              {
                "properties": {
                  "useMap": {
                    "enum": [
                      true
                    ]
                  },
                  "apiKey": {
                    "title": "Your Google Maps API Key",
                    "type": "string"
                  }
                },
                "required": [
                  "apiKey"
                ]
              },
              {
                "properties": {
                  "useMap": {
                    "enum": [
                      false
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    }
  }
}
